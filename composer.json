{
    "name": "aprsoft/yii2-baidu-ueditor",
    "description": "yii2 ueditor extension",
    "type": "yii2-extension",
    "keywords": ["yii2","ueditor"],
    "license": "BSD-4-Clause",
    "authors": [
        {
            "name": "zhangxinyu",
            "email": "176956856@qq.com"
        }
    ], 
    "support": {
        "issues": "https://github.com/AprSoft/yii2-baidu-ueditor/issues",
        "wiki": "https://github.com/AprSoft/yii2-baidu-ueditor/wiki",
        "https://github.com/AprSoft/yii2-baidu-ueditor"
    },
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": "*",
    },
    "repositories":[
        {
            "type": "git",
            "url": "https://github.com/AprSoft/yii2-baidu-ueditor"
        }
    ],
    "autoload": {
        "psr-4": {
            "AprSoft\\ueditor\\": ""
        }
    },
    "config": {
        "process-timeout": 1800,
        "fxp-asset":{
            "installer-paths": {
                "npm-asset-library": "vendor/npm",
                "bower-asset-library": "vendor/bower"
            }
        }
    },
}
